{
    "version": "5.3.1",
    "license": "BSD-3-Clause",
    "homepage": "https://www.anaconda.com/",
    "checkver": {
        "url": "https://repo.continuum.io/archive",
        "re": "Anaconda2-([\\d.]+)-Windows"
    },
    "bin": [
        [
            "python.exe",
            "python2"
        ]
    ],
    "pre_install": "Write-Host 'Installing Anaconda 2. This can take up to 30 minutes on an HDD.' -ForegroundColor Magenta",
    "installer": {
        "file": "anaconda2-installer.exe",
        "args": [
            "/S",
            "/InstallationType=JustMe",
            "/AddToPath=0",
            "/D=$dir"
        ]
    },
    "uninstaller": {
        "file": "Uninstall-Anaconda.exe",
        "args": "/S"
    },
    "env_add_path": "Scripts",
    "architecture": {
        "64bit": {
            "url": "https://repo.continuum.io/archive/Anaconda2-5.3.1-Windows-x86_64.exe#/anaconda2-installer.exe",
            "hash": "md5:ff29ffcd1f767cde91bab71110c00294"
        },
        "32bit": {
            "url": "https://repo.continuum.io/archive/Anaconda2-5.3.1-Windows-x86.exe#/anaconda2-installer.exe",
            "hash": "md5:7286587bcfb6a5a164d70fe02c1968d5"
        }
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://repo.continuum.io/archive/Anaconda2-$version-Windows-x86_64.exe#/anaconda2-installer.exe"
            },
            "32bit": {
                "url": "https://repo.continuum.io/archive/Anaconda2-$version-Windows-x86.exe#/anaconda2-installer.exe"
            }
        },
        "hash": {
            "url": "$baseurl",
            "find": "$basename<.*\\s+<.*\\s+<.*\\s+<td>([a-fA-F0-9]{32})</td>"
        }
    }
}
