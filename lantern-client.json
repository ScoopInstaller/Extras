{
    "homepage": "https://github.com/getlantern/lantern",
    "description": "Lantern is an HTTP/HTTPS proxy, comprised of a desktop/mobile application and a server ecosystem.",
    "version": "5.2.2",
    "license": "Apache-2.0",
    "url": "https://github.com/getlantern/lantern-binaries/raw/master/lantern-installer.exe",
    "hash": "8e7969946ac43aa31bdfb4dce53e676e045213ecd1d7d392648a074ca49b755b",
    "installer": {
        "script": [
            "extract_7zip \"$dir\\lantern-installer.exe\" \"$dir\"",
            "Remove-Item \"$dir\\lantern-installer.exe\"",
            "Remove-Item -Recurse -Force \"$dir\\`$PLUGINSDIR\"",
            "Remove-Item \"$dir\\uninstall.exe.nsis\""
        ]
    },
    "bin": "lantern.exe",
    "shortcuts": [
        [
            "lantern.exe",
            "Lantern",
            "",
            "lantern.ico"
        ]
    ],
    "pre_install": [
        "if (!(Test-Path \"$dir\\settings.yaml\")) {",
        "   New-Item -Force -Path \"$dir\\settings.yaml\" -ItemType file -Value $null | Out-Null",
        "}"
    ],
    "persist": "settings.yaml",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/getlantern/lantern-binaries/raw/master/lantern-installer.exe",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
