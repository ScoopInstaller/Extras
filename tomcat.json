{
    "homepage": "https://tomcat.apache.org/",
    "version": "9.0.6",
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-9/v9.0.6/bin/apache-tomcat-9.0.6-windows-x64.zip",
            "hash": "sha1:800451fb2cf3d7afa6d51d2f09a23c386b0e8d83"
        },
        "32bit": {
            "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-9/v9.0.6/bin/apache-tomcat-9.0.6-windows-x86.zip",
            "hash": "sha1:09095850b4041e670ddca9599ab09744c7468b37"
        }
    },
    "extract_dir": "apache-tomcat-9.0.6",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": [
            "extras/oraclejre-server",
            "openjdk"
        ]
    },
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/",
        "re": "v(?<version>9.[\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip",
                "hash": {
                    "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip.sha1",
                    "find": "([a-fA-F0-9]+)"
                }
            },
            "32bit": {
                "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip",
                "hash": {
                    "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip.sha1",
                    "find": "([a-fA-F0-9]+)"
                }
            }
        },
        "extract_dir": "apache-tomcat-$version"
    }
}
