    "persist": [
        "assets",
        "cores",
        "playlists",
        "records",
        "saves",
        "screenshots",
        "shaders",
        "states",
        "system",
        "thumbnails",
        "retroarch.cfg"
    ],
    "checkver": {
        "url": "http://www.retroarch.com/?page=platforms",
        "re": "The current stable version is: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
