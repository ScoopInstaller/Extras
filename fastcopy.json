{
    "homepage": "https://fastcopy.jp/en/",
    "description": "File and directory copier that runs under Microsoft Windows.",
    "version": "3.54",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://fastcopy.jp/help/license-gpl3.txt"
    },
    "url": "https://fastcopy.jp/archive/FastCopy354_installer.zip",
    "hash": "3a4cf521ed9ff4ceb16dca1695488ba28c6a646bfc9a3b8420175f74a3b436d8",
    "extract_dir": "/",
    "bin": "FastCopy.exe",
    "pre_install": "Get-ChildItem $dir\\*.exe | Rename-Item -NewName {$_.Name -replace $_.Name,'setup_fastcopy.exe'}",
    "installer": {
        "file": "setup_fastcopy.exe",
        "args": [
            "/SILENT",
            "/DIR=$dir"
        ]
    },
    "uninstaller": {
        "file": "setup.exe",
        "args": [
            "/SILENT",
            "/r"
        ]
    },
    "checkver": {
        "url": "https://fastcopy.jp/en/",
        "re": "Download v([\\d\\.]*)"
    },
    "autoupdate": {
        "url": "https://fastcopy.jp/archive/FastCopy$cleanVersion_installer.zip"
    }
}
