{
    "homepage": "https://spring.io/tools/sts",
    "version": "3.9.1",
    "architecture": {
        "64bit": {
            "url": "http://download.springsource.com/release/STS/3.9.1.RELEASE/dist/e4.7/spring-tool-suite-3.9.1.RELEASE-e4.7.1a-win32-x86_64.zip",
            "hash": "sha1:c822859badbb3c2cae76590f3699c6708401a627"
        },
        "32bit": {
            "url": "http://download.springsource.com/release/STS/3.9.1.RELEASE/dist/e4.7/spring-tool-suite-3.9.1.RELEASE-e4.7.1a-win32.zip",
            "hash": "sha1:e868e5796cc130ee1b20b7241e35e5dc8dc462ba"
        }
    },
    "extract_dir": "sts-bundle",
    "pre_install": "mv \"$dir\\sts-$version.RELEASE\" \"$dir\\sts\"",
    "shortcuts": [
        [
            "sts\\STS.exe",
            "Spring Tool Suite"
        ]
    ],
    "bin": "sts\\STS.exe",
    "checkver": {
        "url": "https://spring.io/tools/sts/all",
        "re": "spring-tool-suite-(?<version>[\\d.]+).RELEASE-e(?<eclipse>(?<short>[\\d.]+).[\\d]+)-win32.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://download.springsource.com/release/STS/$matchVersion.RELEASE/dist/e$matchShort/spring-tool-suite-$version.RELEASE-e$matchEclipse-win32-x86_64.zip"
            },
            "32bit": {
                "url": "http://download.springsource.com/release/STS/$matchVersion.RELEASE/dist/e$matchShort/spring-tool-suite-$version.RELEASE-e$matchEclipse-win32.zip"
            }
        },
        "hash": {
            "url": "$url.sha1"
        }
    }
}
