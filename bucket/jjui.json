            "hash": "092cbef0e68949ffc3944a033ab4597ec04de6992450c23bee8867dee54e9366"
        },
        "arm64": {
            "url": "https://github.com/idursun/jjui/releases/download/v0.9.11/jjui-0.9.11-windows-arm64.zip",
            "hash": "638d69b7d388f7b09827b5efdee37caeb6c1b86ce2ec80973a423b8749720fe9"
        }
    },
    "pre_install": "Get-ChildItem -Path $dir -Filter 'jjui*.exe' -Recurse | Select-Object -First 1 | Rename-Item -NewName 'jjui.exe'",
