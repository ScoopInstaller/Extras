    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://dl.grafana.com/oss/release/grafana-12.3.2.windows-amd64.zip",
            "hash": "ab1d4c79abe8a5def0426aa1ac4a9cfba6a9b2ec20274e1d7ee7d460f09434b1"
        },
        "arm64": {
            "url": "https://dl.grafana.com/oss/release/grafana-12.3.2.windows-arm64.zip",
            "hash": "6284b02207107922c5426f7c9198d14e784cf75ca64c8ffc053ce93d02acf4b1"
        }
    },
    "extract_dir": "grafana-12.3.2",
    "bin": [
        "bin\\grafana-cli.exe",
        [
            "bin\\grafana-server.exe",
            "grafana-server",
            "-homepath \"$dir\""
        ]
    ],
    "persist": [
        "conf",
        "data"
    ],
    "checkver": {
        "url": "https://grafana.com/grafana/download?edition=oss&platform=windows",
        "regex": "grafana_([\\d.]+)_(?<stamp>[\\d]+)_windows"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.grafana.com/oss/release/grafana-$version.windows-amd64.zip"
            },
            "arm64": {
                "url": "https://dl.grafana.com/oss/release/grafana-$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        },
        "extract_dir": "grafana-$version"
    }
}
