    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://archive.torproject.org/tor-package-archive/torbrowser/15.0.1/tor-browser-windows-x86_64-portable-15.0.1.exe#/dl.7z",
            "hash": "66793f7208919a15087bac96d8e31151ff53f9620d9fd7bfd340794fa6d5f86c"
        },
        "32bit": {
            "url": "https://archive.torproject.org/tor-package-archive/torbrowser/15.0.1/tor-browser-windows-i686-portable-15.0.1.exe#/dl.7z",
