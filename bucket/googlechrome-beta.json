            "hash": "7676daccc844031215cb7e61a37b11860dc4af728a6cae8bf5f56246c43eb8cf"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/AKJRlumvd2OdHbg8G_VvitE_77.0.3865.35/77.0.3865.35_chrome_installer.exe#/dl.7z",
            "hash": "61651aa8c4dbea70dbaa50ba087a8f9084f0c655051129179ec81df4d4a9efcb"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\chrome.7z\" \"$dir\" -Removal",
            "Move-Item \"$dir\\Chrome-bin\\*\" \"$dir\"",
            "Remove-Item \"$dir\\Chrome-bin\" -Force -Recurse"
        ]
    },
    "bin": [
        [
            "chrome.exe",
            "chrome-beta"
        ]
    ],
    "shortcuts": [
        [
            "chrome.exe",
            "Google Chrome (Beta)"
        ]
    ],
    "checkver": {
        "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
        "regex": "(?sm)<beta32><version>(?<version>[\\d.]+)</version>.+release2/chrome/(?<32>[\\w-]+)_.+<beta64>.+release2/chrome/(?<64>[\\w-]+)_.+</beta64>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/release2/chrome/$match64_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/beta64[version='$version']/sha256"
                }
            },
            "32bit": {
                "url": "https://dl.google.com/release2/chrome/$match32_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/beta32[version='$version']/sha256"
                }
            }
        }
    }
}
