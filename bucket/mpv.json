{
    "version": "0.40.0",
    "description": "Video player based on MPlayer/mplayer2 (builds by shinchiro)",
    "homepage": "https://mpv.io",
    "license": "LGPL-2.1-or-later,GPL-2.0-or-later",
    "notes": "You can use Icaros ('nonportable/icaros-np') to enable thumbnails for all media types.",
    "suggest": {
        "yt-dlp": "yt-dlp"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-0.40.0-x86_64.7z",
            "hash": "sha1:d594f6d686a4323f5c1317a3f60a4c49d6fb60f0"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-0.40.0-i686.7z",
            "hash": "sha1:db61f32941a4fbb471493d0dc4fb3a627b056e17"
        },
        "arm64": {
            "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-0.40.0-aarch64.7z",
            "hash": "sha1:9ad8dd033400ef8b072996e23483ee1e6fd1697a"
        }
    },
    "env_add_path": ".",
    "shortcuts": [
        [
            "mpv.exe",
            "mpv"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "sourceforge": "mpv-player-windows/release",
        "regex": "mpv-([\\d.]+)-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-$version-x86_64.7z"
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-$version-i686.7z"
            },
            "arm64": {
                "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-$version-aarch64.7z"
            }
        }
    }
}
