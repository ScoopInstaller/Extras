{
    "version": "0.36.0",
    "description": "Video player based on MPlayer/mplayer2 (builds by shinchiro)",
    "homepage": "https://mpv.io",
    "license": "LGPL-2.1-or-later,GPL-2.0-or-later",
    "notes": "You can use Icaros ('nonportable/icaros-np') to enable thumbnails for all media types.",
    "suggest": {
        "yt-dlp": "yt-dlp"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-0.36.0-x86_64.7z",
            "hash": "sha1:235279ecb879f79e31aec008b64a2ce22b7f85b9"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-0.36.0-i686.7z",
            "hash": "sha1:50328ea34461989ff514893d70360c335172e9b9"
        }
    },
    "env_add_path": ".",
    "shortcuts": [
        [
            "mpv.exe",
            "mpv"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "sourceforge": "mpv-player-windows/release",
        "regex": "mpv-([\\d.]+)-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-$version-x86_64.7z"
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/mpv-player-windows/release/mpv-$version-i686.7z"
            }
        }
    }
}
