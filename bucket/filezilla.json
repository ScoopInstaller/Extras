    "extract_dir": "FileZilla-3.47.0",
    "pre_install": [
        "$file = 'fzdefaults.xml'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Copy-Item \"$dir\\docs\\fzdefaults.xml.example\" \"$dir\\fzdefaults.xml\"",
        "}"
    ],
