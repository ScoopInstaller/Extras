{
    "version": "5.4.3",
    "description": "Application designed to load test functional behavior and measure performance.",
    "homepage": "https://jmeter.apache.org",
    "license": "Apache-2.0",
    "url": "https://archive.apache.org/dist/jmeter/binaries/apache-jmeter-5.4.3.zip",
    "hash": "sha512:1ebc2a179d724aa58ff8b8f1c2146204208aeeeb8ba2b53168d6700be2d516a204b7e65dd94a6a0e3b84906fd33a97fcf2f2e6e44fb9b8fafa017c0c1856e1d8",
    "extract_dir": "apache-jmeter-5.4.3",
    "bin": [
        "bin\\jmeter.bat",
        "bin\\jmeterw.cmd",
        "bin\\jmeter-n.cmd",
        "bin\\jmeter-n-r.cmd",
        "bin\\jmeter-t.cmd",
        "bin\\jmeter-server.bat",
        "bin\\mirror-server.cmd",
        [
            "bin\\shutdown.cmd",
            "jmeter-shutdown"
        ],
        [
            "bin\\stoptest.cmd",
            "jmeter-stoptest"
        ]
    ],
    "env_set": {
        "JMETER_HOME": "$dir"
    },
    "persist": "lib",
    "checkver": {
        "url": "https://jmeter.apache.org/changes.html",
        "regex": ">Version ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://archive.apache.org/dist/jmeter/binaries/apache-jmeter-$version.zip",
        "hash": {
            "url": "$url.sha512"
        },
        "extract_dir": "apache-jmeter-$version"
    }
}
