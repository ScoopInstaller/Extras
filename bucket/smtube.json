{
    "version": "21.10.0",
    "description": "YouTube browser for SMPlayer.",
    "homepage": "https://www.smtube.org",
    "license": "GPL-2.0-only",
    "depends": "SMPlayer",
    "architecture": {
        "64bit": {
            "url": "https://sourceforge.net/projects/smtube/files/SMTube/21.10.0/smtube-21.10.0-x64.exe#/dl.7z",
            "hash": "sha1:776c7fc98370c22c1bbd640cb93e2a7d28950958"
        },
        "32bit": {
            "url": "https://sourceforge.net/projects/smtube/files/SMTube/21.10.0/smtube-21.10.0-win32-qt5.6.exe#/dl.7z",
            "hash": "sha1:b854f382c3440a1631dcfe9b3caa82cef688e123"
        }
    },
    "installer": {
        "script": [
            "Remove-Item -LiteralPath \"$dir\\`$PLUGINSDIR\" -Force -Recurse",
            "Copy-Item \"$dir\\*\" \"$(appdir SMPlayer $global)\\current\" -Force -Recurse"
        ]
    },
    "checkver": {
        "github": "https://github.com/smplayer-dev/smtube"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://sourceforge.net/projects/smtube/files/SMTube/$version/smtube-$version-x64.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://sourceforge.net/projects/smtube/files/SMTube/$version/smtube-$version-win32-qt5.6.exe#/dl.7z"
            }
        }
    }
}
