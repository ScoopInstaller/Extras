    "installer": {
        "script": [
            "if ((!(Test-Path \"$persist_dir\\ydl-ui.json\")) -and (Test-Path \"$dir\\ydl-ui.json\")) {",
            "    $cont = Get-Content \"$dir\\ydl-ui.json\" | ConvertFrom-Json",
