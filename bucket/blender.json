{
    "version": "4.5.4",
    "description": "3D creation suite",
    "homepage": "https://www.blender.org",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://download.blender.org/release/Blender4.5/blender-4.5.4-windows-x64.zip",
            "hash": "0de55df1d99e4e7152605022cb648e795d5d49209c5c5c4889e1a19fb401a054",
            "extract_dir": "blender-4.5.4-windows-x64"
        },
        "arm64": {
            "url": "https://download.blender.org/release/Blender4.5/blender-4.5.4-windows-arm64.zip",
            "hash": "3a8735b298abaaf8647615026c17c7cdac9d4127356630d08511e6b4beff7fdc",
            "extract_dir": "blender-4.5.4-windows-arm64"
        }
    },
    "bin": "blender-launcher.exe",
    "shortcuts": [
        [
            "blender-launcher.exe",
            "Blender"
        ]
    ],
    "persist": "portable",
    "checkver": {
        "url": "https://download.blender.org/release/",
        "regex": "(\\d+\\.\\d+\\.\\d+)",
        "script": [
            "try {",
            "    if ($expected_ver -notmatch '^\\d+\\.\\d+(\\.\\d+)?$') {",
            "        throw \"$expected_ver not match X.Y.Z\"",
            "    }",
            "    $expectedVersion = [version]$expected_ver",
            "    $expectedBaseVersion = [version]\"$($expectedVersion.Major).$($expectedVersion.Minor).0\"",
            "    $results = (Invoke-WebRequest -Uri \"https://download.blender.org/release/\" -UseBasicParsing).Links | ",
            "    ForEach-Object { ",
            "        if ($_.href -match '^Blender(?<Version>\\d+\\.\\d+)/$') {",
            "            [version]\"$($Matches['Version']).0\"",
            "        }",
            "    } |",
            "    Where-Object { $_ -ge $expectedBaseVersion } |",
            "    Sort-Object -Descending |",
            "    ForEach-Object {",
            "        $versionList = (Invoke-WebRequest -Uri \"https://download.blender.org/release/Blender$($_.Major).$($_.Minor)/\" -UseBasicParsing).Links |",
            "        ForEach-Object { ",
            "            if ($_.href -match '^blender-(?<Version>\\d+\\.\\d+\\.\\d+)-windows-x64\\.zip$') {",
            "                [version]$Matches['Version']",
            "            }",
            "        }",
            "        [PSCustomObject]@{",
            "            VersionList = $versionList",
            "        }",
            "    } |",
            "    Where-Object { $_.VersionList.Count -gt 0 } |",
            "    Select-Object -First 1 |",
            "    ForEach-Object { $_.VersionList }",
            "    $results = $results | Where-Object { $_ -ge $expectedVersion }",
            "    if ($results.Count -eq 0) {",
            "        throw \"No suitable version found >= $expectedVersion\"",
            "    }",
            "    $results = $results | Sort-Object -Descending",
            "    $results -join \"`r`n\"",
            "}",
            "catch {",
            "    Write-Error \"$app checkver failed: $($_.Exception.Message)\"",
            "    return \"\"",
            "}"
        ]
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.blender.org/release/Blender$majorVersion.$minorVersion/blender-$version-windows-x64.zip",
                "extract_dir": "blender-$version-windows-x64"
            },
            "arm64": {
                "url": "https://download.blender.org/release/Blender$majorVersion.$minorVersion/blender-$version-windows-arm64.zip",
                "extract_dir": "blender-$version-windows-arm64"
            }
        },
        "hash": {
            "url": "$baseurl/blender-$version.sha256"
        }
    }
}
