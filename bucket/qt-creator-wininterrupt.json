    "post_install": "New-Item \"$(appdir qt-creator $global)\\current\\bin\\$((Get-Item $dir\\bin\\win*interrupt.exe).Name)\" -ItemType HardLink -Target \"$dir\\bin\\$((Get-Item $dir\\bin\\win*interrupt.exe).Name)\" -Force | Out-Null",
    "checkver": {
