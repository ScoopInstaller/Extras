{
    "version": "2.2.38",
    "description": "All-in-one collaboration platform for API documentation, API debugging, API Mock and API test automation.",
    "homepage": "https://apifox.com",
    "license": "Freeware",
    "architecture": {
        "32bit": {
            "url": "https://cdn.apifox.cn/download/2.2.38/Apifox-windows-x86-2.2.38.exe#/dl.7z",
            "hash": "ec066b416cc4bce529374aa382b8bf5f1f851204e49151786a4d42bc5a542b85",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        },
        "64bit": {
            "url": "https://cdn.apifox.cn/download/2.2.38/Apifox-2.2.38.exe#/dl.7z",
            "hash": "4a65cecd0db4d5eb69cd7a128bef21782420c544e4d5af4e4e749a6584d8fa25",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox"
        ]
    ],
    "checkver": {
        "url": "https://cdn.apifox.cn/download/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-windows-x86-$version.exe#/dl.7z"
            },
            "64bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-$version.exe#/dl.7z"
            }
        }
    },
    "notes": "Your Apifox data are not persisted by Scoop and still storaged in '%APPDATA%\\apifox'."
}
