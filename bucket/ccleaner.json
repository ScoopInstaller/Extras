    "pre_install": [
        "if(!(Test-Path \"$persist_dir\\ccleaner.ini\")) {",
        "   Set-Content \"$dir\\ccleaner.ini\" (@('[Options]', 'UpdateAuto=0', 'UpdateNotify=0', 'UpdateCheck=0', 'HelpImproveCCleaner=0') -join \"`r`n\") -Encoding ASCII",
        "}",
        "'winapp.ini', 'winreg.ini', 'winsys.ini' | ForEach-Object {",
        "   if(!(Test-Path \"$persist_dir\\$_\")) { New-Item \"$dir\\$_\" | Out-Null }",
        "}"
    ],
    "installer": {
        "script": [
            "if (Test-Path \"$persist_dir\\branding.dll\") { Copy-Item \"$persist_dir\\branding.dll\" \"$dir\" -Force }",
            "if (Test-Path \"$persist_dir\\CCleaner.dat\") { Copy-Item \"$persist_dir\\CCleaner.dat\" \"$dir\" -Force }"
        ]
    },
    "uninstaller": {
        "script": [
            "if (Test-Path \"$dir\\branding.dll\") { Copy-Item \"$dir\\branding.dll\" \"$persist_dir\" -Force }",
            "if (Test-Path \"$dir\\CCleaner.dat\") { Copy-Item \"$dir\\CCleaner.dat\" \"$persist_dir\" -Force }"
        ]
    },
    "persist": [
        "ccleaner.ini",
        "winapp.ini",
        "winreg.ini",
        "winsys.ini"
