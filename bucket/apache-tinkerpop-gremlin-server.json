{
    "version": "3.7.4",
    "description": "Remotely execute Gremlin against one or more Graph instances hosted within it.",
    "homepage": "https://tinkerpop.apache.org",
    "license": "Apache-2.0",
    "url": "https://downloads.apache.org/tinkerpop/3.7.4/apache-tinkerpop-gremlin-server-3.7.4-bin.zip",
    "hash": "sha512:87a483d6780c89b88a95e2e9f4f03aa08bcd0a149250972f1c50112ecfef7e4d89a6fc987858da7c6766de068b3be1031aed81e2597f6e9931cc3a6f27f578ce",
    "extract_dir": "apache-tinkerpop-gremlin-server-3.7.4",
    "pre_install": [
        "$cont = @(",
        "    '@echo off'",
        "    'cd /d \"%~dp0\"'",
        "    'gremlin-server.bat'",
        ")",
        "Set-Content \"$dir\\bin\\gremlin-server-scoop.bat\" $cont -Encoding ascii"
    ],
    "bin": [
        [
            "bin\\gremlin-server-scoop.bat",
            "gremlin-server"
        ]
    ],
    "checkver": {
        "url": "https://downloads.apache.org/tinkerpop/",
        "regex": "href=\"([\\d.]+)/",
        "reverse": true
    },
    "autoupdate": {
        "url": "https://downloads.apache.org/tinkerpop/$version/apache-tinkerpop-gremlin-server-$version-bin.zip",
        "hash": {
            "url": "$url.sha512"
        },
        "extract_dir": "apache-tinkerpop-gremlin-server-$version"
    }
}
