    "extract_dir": "ghidra_10.0.1_PUBLIC",
    "suggest": {
        "JDK": "java/adoptopenjdk-lts-hotspot"
    },
    "notes": "Must be with JDK 11 64-bit installed",
    "architecture": {
        "64bit": {
            "url": "https://ghproxy.com/https://github.com/NationalSecurityAgency/ghidra/releases/download/Ghidra_10.0.1_build/ghidra_10.0.1_PUBLIC_20210708.zip",
            "hash": "9b68398fcc4c2254a3f8ff231c4e8b2ac75cc8105f819548c7eed3997f8c5a5d"
        }
    },
    "bin": [
        "ghidraRun.bat"
    ],
    "shortcuts": [
        [
            "ghidraRun.bat",
            "ghidra"
        ]
    ],
    "checkver": {
        "github": "https://github.com/NationalSecurityAgency/ghidra",
        "regex": "Ghidra_([\\w.]+)_build/ghidra_(?<full>[\\w.]+).zip"
    },
    "autoupdate": {
