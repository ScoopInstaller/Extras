            "64bit": {
                "url": "https://github.com/git-for-windows/git/releases/download/Ghidra_version_build/ghidra_matchFull.zip"
            }
        },
        "hash": {
            "url": "https://github.com/NationalSecurityAgency/ghidra/releases/tag/Ghidra_$version_build",
            "regex": "<li>SHA-256: </li>\\s*<code>$sha256</code>"
        },
        "extract_dir": "ghidra_$version_PUBLIC"
    }
}
