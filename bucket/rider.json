{
    "version": "2023.3.1-233.11799.303",
    "description": "Cross-Platform IDE for .NET by JetBrains.",
    "homepage": "https://www.jetbrains.com/rider/",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/rider/JetBrains.Rider-2023.3.1.win.zip",
            "hash": "f760bb85240609701d3014c9c7eea5308cd4b04fb7c010f24245170e7258b6e4"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/rider/JetBrains.Rider-2023.3.1-aarch64.win.zip",
            "hash": "bfc955f65800f29f119ec43750c4956991aaf5a52bfba18e5b4f72ceb9ec7e6b"
        }
    },
    "extract_to": "IDE",
    "installer": {
        "script": "& \"$bucketsdir\\extras\\scripts\\jetbrains\\portable.ps1\" \"$dir\" \"$persist_dir\""
    },
    "bin": [
        [
            "IDE\\bin\\rider64.exe",
            "rider"
        ]
    ],
    "shortcuts": [
        [
            "IDE\\bin\\rider64.exe",
            "JetBrains\\Rider"
        ]
    ],
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=RD&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/rider/JetBrains.Rider-$matchHead.win.zip"
            },
            "arm64": {
                "url": "https://download.jetbrains.com/rider/JetBrains.Rider-$matchHead-aarch64.win.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
