{
    "version": "1.2.3.1",
    "description": "Creates an overlay for running GPU shaders on top of the Windows desktop.",
    "homepage": "https://github.com/mausimus/ShaderGlass",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/mausimus/ShaderGlass/releases/download/v$matchTag/ShaderGlass-$version-win-x64.zip",
            "hash": "sha256:$hash"
        }
    },
    "bin": "ShaderGlass.exe",
    "shortcuts": [
        ["ShaderGlass.exe", "ShaderGlass"]
    ],
    "checkver": {
        "github": "https://github.com/mausimus/ShaderGlass",
        "regex": "ShaderGlass-([\\d.]+)-win-x64\\.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/mausimus/ShaderGlass/releases/download/v$matchTag/ShaderGlass-$version-win-x64.zip"
            }
        }
    }
}
