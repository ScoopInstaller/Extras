    "version": "1.15.1",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/appium/appium-desktop/releases/download/v1.15.1/Appium-windows-1.15.1.exe#/dl.7z",
            "hash": "sha512:33b30a4a23c6a34be7f714148357b98e184788c695f11bd34172644a9ef5b695be23d71968a5516be235b326d4eaad2c3c3b715d1ce8a3a4a583d8ac655b375e",
            "installer": {
                "script": [
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninst*\" -Force -Recurse"
                ]
            }
        }
    },
    "shortcuts": [
        [
            "Appium.exe",
            "Appium"
        ]
    ],
    "checkver": {
        "github": "https://github.com/appium/appium-desktop"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/appium/appium-desktop/releases/download/v$version/Appium-windows-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "find": "sha512:\\s+(.*)"
                }
            }
        }
    }
}
