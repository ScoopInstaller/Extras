    "bin": [
        "capnp.exe",
        "capnpc-c++.exe",
        "capnpc-capnp.exe"
    ],
    "checkver": {
        "url": "https://capnproto.org/install.html",
        "regex": "<a href=\"https://capnproto.org/capnproto-c\\+\\+-win32-([\\d.]+).zip"
    },
    "autoupdate": {
        "url": "https://capnproto.org/capnproto-c++-win32-$version.zip"
    }
