    "description": "GitHub notifications on menu bar",
    "homepage": "https://www.gitify.io",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/gitify-app/gitify/releases/download/v5.5.0/Gitify-Setup-5.5.0.exe#/dl.7z",
            "hash": "sha512:e6013fa2fdf8e5b24cf088c70375c20e7544d5ff11c4d1bd1124a8cd20a153d1bcfc430ca642bd4d92a2ff81bc5c90148fdb99a9f95bdf8e63d362b46e53ca0c",
            "pre_install": [
