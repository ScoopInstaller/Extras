{
    "version": "6.1.0",
    "description": "GitHub notifications on menu bar",
    "homepage": "https://www.gitify.io",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/gitify-app/gitify/releases/download/v6.1.0/Gitify-Setup-6.1.0.exe#/dl.7z",
            "hash": "sha512:05ca304ca214c86860034af65bb4040282e98322060f9a4f36747a3abc372f9cd2acc018314ec20bcc9d607967bc81aae5e34affa5a696f7e2bb90a5bc9b988c",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal",
