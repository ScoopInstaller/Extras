{
    "homepage": "https://github.com/stax76/mpv.net/",
    "description": "A modern media player for Windows that works just like mpv",
    "license": "MIT",
    "version": "4.7.1",
    "architecture": {
        "64bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.7.1/mpv.net-portable-x64-4.7.1.0.7z",
            "hash": "a4b5a1ac7c0d8d58ef9e46685923baaf0f28e1d97317e86ef0e5534cfbcb07f4"
        },
        "32bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/4.7.1/mpv.net-portable-x86-4.7.1.0.7z",
            "hash": "11b19197a74dd168f8db4ba8bd08f172cd2cc334a242359ed7b3b093a71f48be"
        }
    },
    "bin": "mpvnet.exe",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/stax76/mpv.net",
        "regex": "download/(?<main>[\\d.]+)/mpv.net-portable-x64-(?<full>[\\d.]+)\\.7z",
        "replace": "${main}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x64-$matchFull.7z"
            },
            "32bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x86-$matchFull.7z"
            }
        }
    }
}
