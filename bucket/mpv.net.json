{
    "homepage": "https://github.com/stax76/mpv.net/",
    "description": "A modern media player for Windows that works just like mpv",
    "license": "MIT",
    "version": "5.2",
    "architecture": {
        "64bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/5.2/mpv.net-portable-x64-5.2.0.0.7z",
            "hash": "4c517b59b6ad9255aabc5c7e363a104b3d0d69040427958a9bfe29828a6dd69f"
        },
        "32bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/5.2/mpv.net-portable-x86-5.2.0.0.7z",
            "hash": "837f766cfd20ee834e865c5f919929542a4473e77ead453c9fd064aadfd4064a"
        }
    },
    "bin": "mpvnet.exe",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/stax76/mpv.net",
        "regex": "download/(?<main>[\\d.]+)/mpv.net-portable-x64-(?<full>[\\d.]+)\\.7z",
        "replace": "${main}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x64-$matchFull.7z"
            },
            "32bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x86-$matchFull.7z"
            }
        }
    }
}
