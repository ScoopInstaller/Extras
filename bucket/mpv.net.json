{
    "homepage": "https://github.com/stax76/mpv.net/",
    "description": "A modern media player for Windows that works just like mpv",
    "license": "MIT",
    "version": "5.4.3.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/5.4.3.0/mpv.net-portable-x64-5.4.3.0.7z",
            "hash": "feeacd13368597818dfe10e4075643f643dda2204cb7fcc947591b54936ac939"
        },
        "32bit": {
            "url": "https://github.com/stax76/mpv.net/releases/download/5.4.3.0/mpv.net-portable-x86-5.4.3.0.7z",
            "hash": "38a5eb85a992141c344ad6db6cc21fef0801186d9dae9900d8a7fde347525991"
        }
    },
    "bin": "mpvnet.exe",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/stax76/mpv.net",
        "regex": "download/(?<main>[\\d.]+)/mpv.net-portable-x64-(?<full>[\\d.]+)\\.7z",
        "replace": "${main}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x64-$matchFull.7z"
            },
            "32bit": {
                "url": "https://github.com/stax76/mpv.net/releases/download/$matchMain/mpv.net-portable-x86-$matchFull.7z"
            }
        }
    }
}
