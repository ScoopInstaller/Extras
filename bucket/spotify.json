{
    "version": "nightly",
    "description": "Client for spotify music streaming service",
    "homepage": "https://www.spotify.com",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.spotify.com/au/legal/end-user-agreement"
    },
    "url": "https://download.spotify.com/SpotifyFullSetup.exe",
    "installer": {
        "args": [
            "/extract",
            "\"$dir\""
        ]
    },
    "uninstaller": {
        "file": "Spotify.exe",
        "args": [
            "/Uninstall",
            "/Silent"
        ]
    },
    "shortcuts": [
        [
            "Spotify.exe",
            "Spotify"
        ]
    ]
}
