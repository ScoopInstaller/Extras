    "shortcuts": [
        [
            "MicroSIP.exe",
            "MicroSIP"
        ]
    ],
    "persist": [
        "Contacts.xml",
        "MicroSIP.ini"
    ],
    "checkver": {
        "url": "https://www.microsip.org/downloads",
        "regex": "MicroSIP-([\\d.]+)\\.zip"
    },
    "autoupdate": {
        "url": "https://www.microsip.org/download/MicroSIP-$version.zip"
    }
