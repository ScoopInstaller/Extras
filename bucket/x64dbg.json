        "}"
    ],
    "bin": [
        "release\\x96dbg.exe",
        "release\\x32\\x32dbg.exe",
        "release\\x64\\x64dbg.exe"
    ],
    "shortcuts": [
        [
            "release\\x96dbg.exe",
            "x96dbg"
        ],
        [
            "release\\x32\\x32dbg.exe",
            "x32dbg"
        ],
        [
            "release\\x64\\x64dbg.exe",
            "x64dbg"
        ]
    ],
    "persist": [
        "release\\x96dbg.ini",
        "release\\x32\\x32dbg.ini",
        "release\\x32\\memdumps",
        "release\\x32\\garbage",
        "release\\x32\\db",
        "release\\x32\\plugins",
        "release\\x64\\x64dbg.ini",
        "release\\x64\\memdumps",
        "release\\x64\\garbage",
        "release\\x64\\db",
        "release\\x64\\plugins"
    ],
    "checkver": {
        "github": "https://github.com/x64dbg/x64dbg",
        "regex": "snapshot_([\\d-_]+)"
    },
    "autoupdate": {
