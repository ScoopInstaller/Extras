{
    "homepage": "https://openmpt.org/",
    "description": "Music tracker",
    "license": "BSD-3-Clause",
    "version": "1.28.10.00",
    "architecture": {
        "32bit": {
            "url": "https://download.openmpt.org/archive/openmpt/1.28/OpenMPT-1.28.10.00-Setup.exe",
            "hash": "sha1:16883b9955ee4cc88614aca63bbeb68581f9ed0e"
        },
        "64bit": {
            "url": "https://download.openmpt.org/archive/openmpt/1.28/OpenMPT-1.28.10.00-Setup-x64.exe",
            "hash": "sha1:dedd2e77c1d4705b7ed144fa05dad8eca6e43087"
        }
    },
    "innosetup": true,
    "pre_install": [
        "Remove-Item \"$dir\\*,2*\" -Force",
        "Get-ChildItem \"$dir\" '*,1*' | ForEach-Object { Rename-Item $_.Fullname \"$($_.Basename.TrimEnd(',1'))$($_.Extension)\" }"
    ],
    "shortcuts": [
        [
            "mptrack.exe",
            "OpenMPT"
        ]
    ],
    "checkver": {
        "url": "https://openmpt.org/download",
        "regex": "OpenMPT ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://download.openmpt.org/archive/openmpt/$majorVersion.$minorVersion/OpenMPT-$version-Setup.exe"
            },
            "64bit": {
                "url": "https://download.openmpt.org/archive/openmpt/$majorVersion.$minorVersion/OpenMPT-$version-Setup-x64.exe"
            }
        },
        "hash": {
            "url": "$url.digests",
            "regex": "$sha1"
        }
    }
}
