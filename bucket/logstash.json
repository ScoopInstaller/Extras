            "bin\\logstash.bat",
            "logstash"
        ],
        [
            "bin\\logstash-plugin.bat",
            "logstash-plugin"
        ]
    ],
    "persist": [
        "config",
        "data"
    ],
    "checkver": {
        "url": "https://www.elastic.co/downloads/logstash",
        "regex": "logstash-([\\d.]+)-windows-x86_64\\.zip"
    },
    "autoupdate": {
