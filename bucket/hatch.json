{
    "version": "1.7.0",
    "description": "Hatch is a modern, extensible Python project manager.",
    "homepage": "https://github.com/pypa/hatch",
    "license": "MIT",
    "depends": "python",
    "url": "https://files.pythonhosted.org/packages/py3/h/hatch/hatch-1.7.0-py3-none-any.whl",
    "hash": "efc84112fd02ca85b7bab54f5e2ef71393a98dc849eac9aca390504031f8a1a8",
    "installer": {
        "script": [
            "Push-Location \"$dir\"",
            "python -m venv hatch",
            ".\\hatch\\Scripts\\activate",
            "try {",
            "    python -m pip install \"$fname\"",
            "}",
            "finally {",
            "    Remove-Item \"$fname\"",
            "    deactivate",
            "    Pop-Location",
            "}"
        ]
    },
    "bin": "hatch\\Scripts\\hatch.exe",
    "checkver": {
        "github": "https://github.com/pypa/hatch",
        "regex": "/releases/tag/hatch-(?:v|V)?([\\d\\.]+)"
    },
    "autoupdate": {
        "url": "https://files.pythonhosted.org/packages/py3/h/hatch/hatch-$version-py3-none-any.whl"
    }
}
