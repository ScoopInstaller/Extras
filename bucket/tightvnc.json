        }
    },
    "pre_install": [
        "Move-Item \"$dir\\PFiles\\TightVNC\\*\" \"$dir\\\"",
        "Remove-Item \"$dir\\PFiles\" -Force -Recurse"
    ],
    "bin": ["tvnviewer.exe","tvnserver.exe"],
    "shortcuts": [
        [
            "tvnviewer.exe",
            "TighVNC Viewer"
        ],
         [
            "tvnserver.exe",
            "TighVNC Server"
        ]
    ],
    "checkver": {
        "url": "https://www.tightvnc.com/download.php",
        "regex": "Download TightVNC for Windows (Version ([\\d.]+))"
    },
    "autoupdate": {
        "32bit": {
            "url": "https://www.tightvnc.com/download/$version/tightvnc-$version-gpl-setup-32bit.msi"
        },       
        "64bit": {
            "url": "https://www.tightvnc.com/download/$version/tightvnc-$version-gpl-setup-64bit.msi"
