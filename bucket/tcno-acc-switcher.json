{
    "version": "5.0-2022-03-29_00",
    "description": "A super fast account switcher for Steam",
    "homepage": "https://github.com/TcNobo/TcNo-Acc-Switcher",
    "license": "GPL-3.0-only,LGPL-2.1-only,...",
    "notes": ".NET Framework 4.8 is required.",
    "url": "https://github.com/TcNobo/TcNo-Acc-Switcher/releases/download/2022-03-29_00/TcNo-Acc-Switcher_2022-03-29_00.7z",
    "hash": "4fcd88adf10634d454a89c5d33ef54e052622aa53eb98ceb3100ab4204792c7b",
    "pre_install": [
        "'globals.json' | ForEach-Object {",
        "    $cont = if ($_ -like '*Tray_Users*') { '[]' } else { '{}' }",
        "    if (!(Test-Path \"$persist_dir\\$_\")) { Set-Content \"$dir\\$_\" $cont -Encoding Ascii | Out-Null }",
        "}"
    ],
    "shortcuts": [
        [
            "TcNo-Acc-Switcher.exe",
            "TcNo Account Switcher"
        ]
    ],
    "persist": [
        "Errors",
        "globals.json",
        "Steam\\SteamSettings.json",
        "Steam\\SteamVACCache.json",
        "Steam\\Tray_Users.json",
        "Steam\\Errors",
        "Steam\\images"
    ],
    "checkver": {
        "github": "https://github.com/TcNobo/TcNo-Acc-Switcher",
        "regex": "TcNo Account Switcher - v([\\d.]+) - (\\d{4}-\\d{2}-\\d{2}_\\d{2})",
        "replace": "${1}-${2}"
    },
    "autoupdate": {
        "url": "https://github.com/TcNobo/TcNo-Acc-Switcher/releases/download/$match2/TcNo-Acc-Switcher_$match2.7z"
    }
}
