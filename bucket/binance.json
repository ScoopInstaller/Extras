    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://ftp.binance.com/electron-desktop/windows/production/binance-setup-$version.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "https://ftp.binance.com/electron-desktop/windows/production/latest.yml",
