        "xpath": "/updates/update/@appVersion"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$versionesr/win64/en-US/Firefox%20Setup%20$versionesr.exe#/firefox.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$versionesr/SHA256SUMS",
                    "regex": "$sha256\\s+win64/en-US/Firefox Setup"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$versionesr/win32/en-US/Firefox%20Setup%20$versionesr.exe#/firefox.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$versionesr/SHA256SUMS",
                    "regex": "$sha256\\s+win32/en-US/Firefox Setup"
                }
            }
        }
    }
