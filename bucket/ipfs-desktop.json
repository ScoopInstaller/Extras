{
    "version": "0.22.0",
    "description": "A desktop client for IPFS.",
    "homepage": "https://github.com/ipfs/ipfs-desktop",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/ipfs/ipfs-desktop/releases/download/v0.22.0/ipfs-desktop-setup-0.22.0.exe#/dl.7z",
            "hash": "sha512:3fac173fb5d32a42bb499827afe748964beb118ab5359ab53c0bde9ef6465bbd3b3b44d2308a287e93156e0dedee1b5b387b7ff3871e57638329a670a0e3f43e",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$PLUGINSDIR\" -Recurse"
            ]
        }
    },
    "bin": "resources\\app.asar.unpacked\\node_modules\\go-ipfs\\go-ipfs\\ipfs.exe",
    "shortcuts": [
        [
            "IPFS Desktop.exe",
            "IPFS Desktop"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ipfs/ipfs-desktop/releases/download/v$version/ipfs-desktop-setup-$version.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512:\\s+$base64"
        }
    }
}
