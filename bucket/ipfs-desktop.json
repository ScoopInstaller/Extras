    "description": "A desktop client for IPFS.",
    "homepage": "https://github.com/ipfs/ipfs-desktop",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/ipfs/ipfs-desktop/releases/download/v0.25.0/ipfs-desktop-setup-0.25.0.exe#/dl.7z",
            "hash": "sha512:2573ea3389f5a081a02e2f27d5ba538aeab207daa20d86f6eb07e84fcfabfab90f4aed20377b7ae64cec2fcd978f741b71b823a45622cc1a8176f103dca6aed9",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
