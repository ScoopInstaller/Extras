            "Crow Translate"
        ]
    ],
    "checkver": {
        "url": "https://apps.kde.org/crowtranslate",
        "regex": "stable/crow-translate/([\\d.]+)/crow-translate-release_(?<full>[\\d.-]+)-windows-cl-(?<lib>\\w+)-x86_64\\.7z"
    },
    "autoupdate": {
