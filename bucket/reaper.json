    },
    "notes": "The portable version of Reaper does not include ReWire.",
    "architecture": {
        "64bit": {
            "url": "https://www.reaper.fm/files/6.x/reaper670_x64-install.exe#/dl.7z",
            "hash": "47e3c4f24caf60f13ff72e24d9044af7cebc72d73f95f348725779989ed4b2a8"
        },
        "32bit": {
            "url": "https://www.reaper.fm/files/6.x/reaper670-install.exe#/dl.7z",
            "hash": "65bdff49406e86237c2a1364518d31dfdace5a70ba105409ef5dd107bba42ee2"
        }
    },
    "bin": [
        "reaper.exe",
        "reamote.exe"
    ],
    "shortcuts": [
        [
            "reaper.exe",
            "Reaper"
        ],
        [
            "reamote.exe",
            "ReaMote"
        ]
    ],
    "installer": {
        "script": [
            "Rename-Item \"$dir\\`$INSTDIR`$_8_\" 'InstallData'",
            "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse -Force"
        ]
    },
    "checkver": {
        "url": "https://www.reaper.fm/download.php",
        "regex": "Version ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.reaper.fm/files/$majorVersion.x/reaper$cleanVersion_x64-install.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://www.reaper.fm/files/$majorVersion.x/reaper$cleanVersion-install.exe#/dl.7z"
            }
        }
    }
}
