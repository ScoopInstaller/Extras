        "indexes",
        "logs",
        "scripts",
        "update",
        "sdi.cfg"
    ],
    "checkver": {
        "url": "https://www.snappy-driver-installer.org/download/",
        "regex": "SDIO_([\\d.]+).zip"
    },
    "autoupdate": {
        "url": "https://snappy-driver-installer.org/downloads/SDIO_$version.zip"
