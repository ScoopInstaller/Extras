    "uninstaller": {
        "file": "Uninstall.exe"
    },
    "checkver": {
        "github": "https://github.com/bitbeans/SimpleDnsCrypt"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/bitbeans/SimpleDnsCrypt/releases/download/$version/SimpleDNSCrypt64.msi",
                "hash": {"url": "$url.sha512"},
                "post_install": [
                    "if (Test-Path \"$dir\\SimpleDNSCrypt64.msi\") {Remove-Item \"$dir\\SimpleDNSCrypt64.msi\" | Out-Null}"
