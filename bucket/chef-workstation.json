        ]
    ],
    "checkver": {
        "url": "https://docs.chef.io/release_notes_workstation/",
        "regex": "Chef Workstation ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://packages.chef.io/files/stable/chef-workstation/$version/windows/2022/chef-workstation-$version-1-x64.msi",
