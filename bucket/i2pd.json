            "hash": "35d9b619b56fd0e5c959a04f25396ead6b869448ee5dc1fd3282cb04fc0eb28e"
        },
        "32bit": {
            "url": "https://github.com/PurpleI2P/i2pd/releases/download/2.24.0/i2pd_2.24.0_win32_mingw.zip",
            "hash": "011ebcd5d59a83bf869d56320db74cee75ed3bafc8c1d6289a48736f8441c6ba"
        }
    },
    "bin": "i2pd.exe",
    "shortcuts": [
        [
            "i2pd.exe",
            "I2Pd"
        ]
    ],
    "persist": [
        "addressbook",
        "destinations",
        "netDb",
        "peerProfiles",
        "tags",
        "irc-keys.dat",
        "ntcp2.keys",
        "router.info",
        "router.keys",
        "certificates",
        "tunnels.d",
        "i2pd.conf",
        "tunnels.conf"
    ],
    "pre_install": [
        "Move-Item \"$dir\\contrib\\*\" \"$dir\"",
        "Remove-Item \"$dir\\contrib\"",
        "foreach ($name in @('irc-keys.dat', 'ntcp2.keys', 'router.info', 'router.keys')) {",
        "    if (!(Test-Path \"$persist_dir\\$name\")) {",
        "        New-Item -Force -Path \"$dir\" -Name $name | Out-Null",
        "    }",
        "}"
    ],
    "checkver": {
        "github": "https://github.com/PurpleI2P/i2pd"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/PurpleI2P/i2pd/releases/download/$version/i2pd_$version_win64_mingw.zip"
            },
            "32bit": {
                "url": "https://github.com/PurpleI2P/i2pd/releases/download/$version/i2pd_$version_win32_mingw.zip"
            }
        }
    }
}
