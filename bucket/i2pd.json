{
    "version": "2.32.1",
    "description": "End-to-End encrypted and anonymous Internet",
    "homepage": "https://i2pd.website",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://github.com/PurpleI2P/i2pd/releases/download/2.32.1/i2pd_2.32.1_win64_mingw.zip",
            "hash": "191341dedba16a63166b1fd92628e8560ecf46ec68f8db51a88288ba6713724a"
        },
        "32bit": {
            "url": "https://github.com/PurpleI2P/i2pd/releases/download/2.32.1/i2pd_2.32.1_win32_mingw.zip",
            "hash": "387082a81a0eed5f0f95b974fc2e533a78bf8bebaeab2821007ce541a5ee206e"
        }
    },
    "bin": "i2pd.exe",
    "shortcuts": [
        [
            "i2pd.exe",
            "I2Pd"
        ]
    ],
    "persist": [
        "addressbook",
        "destinations",
        "netDb",
        "peerProfiles",
        "tags",
        "irc-keys.dat",
        "ntcp2.keys",
        "router.info",
        "router.keys",
        "certificates",
        "tunnels.d",
        "i2pd.conf",
        "tunnels.conf"
    ],
    "pre_install": [
        "Move-Item \"$dir\\contrib\\*\" \"$dir\"",
        "Remove-Item \"$dir\\contrib\"",
        "foreach ($name in @('irc-keys.dat', 'ntcp2.keys', 'router.info', 'router.keys')) {",
        "    if (!(Test-Path \"$persist_dir\\$name\")) {",
        "        New-Item -Force -Path \"$dir\" -Name $name | Out-Null",
        "    }",
        "}"
    ],
    "checkver": {
        "github": "https://github.com/PurpleI2P/i2pd"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/PurpleI2P/i2pd/releases/download/$version/i2pd_$version_win64_mingw.zip"
            },
            "32bit": {
                "url": "https://github.com/PurpleI2P/i2pd/releases/download/$version/i2pd_$version_win32_mingw.zip"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512SUMS"
        }
    }
}
