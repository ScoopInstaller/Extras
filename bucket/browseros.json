{
    "version": "0.34.0",
    "description": "BrowserOS is an open-source chromium fork that runs AI agents natively. Your open-source, privacy-first alternative to ChatGPT Atlas, Perplexity Comet, Dia.",
    "homepage": "https://github.com/browseros-ai/BrowserOS",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/browseros-ai/BrowserOS/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/browseros-ai/BrowserOS/releases/download/v0.34.0/BrowserOS_v0.34.0_x64_installer.exe#/dl.7z",
            "hash": "e94b215fe2f54a811a15baee53b7f48f51b4ebbbb4bf766a6ffd3bd43ad64b9b"
        }
    },
    "installer": {
        "script": "Expand-7zipArchive -Path \"$dir\\chrome.7z\" -DestinationPath \"$dir\" -ExtractDir \"Chrome-bin\" -Removal"
    },
    "post_install": [
        "if (!(Test-Path \"$dir\\User Data\\*\") -and (Test-Path \"$env:LocalAppData\\BrowserOS\\BrowserOS\\User Data\")) {",
        "    info '[Portable Mode]: Copying user data...'",
        "    Copy-Item \"$env:LocalAppData\\BrowserOS\\BrowserOS\\User Data\\*\" \"$dir\\User Data\" -Recurse",
        "}"
    ],
    "shortcuts": [
        [
            "chrome.exe",
            "BrowserOS",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/browseros-ai/BrowserOS/releases/download/v$majorVersion.$minorVersion.$patchVersion/BrowserOS_v$version_x64_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://api.github.com/repos/browseros-ai/BrowserOS/releases/tags/v$majorVersion.$minorVersion.$patchVersion",
                    "regex": "sha256:([0-9a-z]+)[^}]+$url"
                }
            }
        }
    }
}
