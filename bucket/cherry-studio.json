        "arm64": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.7.0/Cherry-Studio-1.7.0-arm64-setup.exe#/dl.7z",
            "hash": "e3fc0d44f363b6aa7701a480c3e6d73283ec1fab1d0ebb5ba765bf9c3c900956",
