{
    "version": "3.6.0",
    "description": "A REPL environment for interactive development and analysis of Gremlin applications.",
    "homepage": "https://tinkerpop.apache.org",
    "license": "Apache-2.0",
    "url": "https://www.apache.org/dist/tinkerpop/3.6.0/apache-tinkerpop-gremlin-console-3.6.0-bin.zip",
    "hash": "sha512:727061ee2874087bc889cd26aa651537c96eaa84d9eaed568902de67d0765a9acca4cb6efa11786a45e6fdde5242df908ab2d84ea925dd87ae907537949b108a",
    "extract_dir": "apache-tinkerpop-gremlin-console-3.6.0",
    "pre_install": [
        "$cont = @(",
        "    '@echo off'",
        "    'cd /d \"%~dp0\"'",
        "    'gremlin.bat'",
        ")",
        "Set-Content \"$dir\\bin\\gremlin-scoop.bat\" $cont -Encoding ascii"
    ],
    "bin": [
        [
            "bin\\gremlin-scoop.bat",
            "gremlin"
        ]
    ],
    "checkver": {
        "url": "https://www.apache.org/dist/tinkerpop/",
        "regex": "href=\"([\\d.]+)/",
        "reverse": true
    },
    "autoupdate": {
        "url": "https://www.apache.org/dist/tinkerpop/$version/apache-tinkerpop-gremlin-console-$version-bin.zip",
        "hash": {
            "url": "$url.sha512"
        },
        "extract_dir": "apache-tinkerpop-gremlin-console-$version"
    }
}
