        [
            "bin\\sysgau.exe",
            "SysGauge"
        ]
    ],
    "checkver": {
        "url": "https://www.sysgauge.com/downloads.html",
        "regex": "(?sm)>SysGauge.*?>([\\d.]+)<"
    },
