    },
    "shortcuts": [
        [
            "bin\\sysgau.exe",
            "SysGauge"
        ]
    ],
    "checkver": {
        "url": "https://www.sysgauge.com/downloads.html",
        "regex": "<b>SysGauge</b><br>v([\\d.]+)"
    },
