            "LabPlot2"
        ]
    ],
    "checkver": {
        "url": "https://labplot.kde.org/download/",
        "regex": "labplot-([\\d.]+)\\.exe"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.kde.org/stable/labplot/labplot-$version.exe#/dl.7z"
            }
        }
    }
