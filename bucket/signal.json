{
    "version": "6.0.1",
    "description": "A cross-platform encrypted messaging service.",
    "homepage": "https://signal.org",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://updates.signal.org/desktop/signal-desktop-win-6.0.1.exe#/dl.7z",
            "hash": "sha512:8e3c2dfc9b3b6a90f6df56b98f3c3447552a5aa5c2e6a7a7c78e6ea5ed6661d7d38612dc2bccea981cebedf5414d0c63bfc61aca7ff5340354cc06ff75a5f873",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
