        "    }",
        "}"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "FileOptimizer64.exe",
                    "FileOptimizer"
                ]
            ],
            "shortcuts": [
                [
                    "FileOptimizer64.exe",
                    "FileOptimizer"
                ]
            ]
        },
        "32bit": {
            "bin": [
                [
                    "FileOptimizer32.exe",
                    "FileOptimizer"
                ]
            ],
            "shortcuts": [
                [
                    "FileOptimizer32.exe",
                    "FileOptimizer"
                ]
            ]
        }
    },
    "persist": [
        "FileOptimizer32.ini",
        "FileOptimizer64.ini"
    ],
    "checkver": {
        "url": "https://sourceforge.net/projects/nikkhokkho/files/FileOptimizer/",
        "regex": "FileOptimizer/([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://downloads.sourceforge.net/project/nikkhokkho/FileOptimizer/$version/FileOptimizerFull.7z.exe#/dl.7z"
    }
