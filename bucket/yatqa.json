    "homepage": "https://yat.qa/",
    "license": {
        "identifier": "Freeware",
        "url": "https://referencesource.microsoft.com/license.html"
    },
    "url": "https://dl.yat.qa/stable/YaTQA-Setup_3.9.8.3f.exe",
