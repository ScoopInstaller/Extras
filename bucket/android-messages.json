    "architecture": {
        "64bit": {
            "url": "https://github.com/OrangeDrangon/android-messages-desktop/releases/download/v5.6.0/Android-Messages-v5.6.0-win-x64.portable.exe#/dl.7z",
            "hash": "sha512:21acb1121048d45dde0b8fdea07114dfe19ca5cccf4f131f4bc314eb096823c9918bb3b51321aa50f363db9a784d01a3d7a0e55f59ece8831da20315eff78e4c",
            "extract_dir": "$PLUGINSDIR"
