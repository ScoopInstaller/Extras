            "hash": "5673cb63f438981930d33056be7aeee3f222310de94bea8eefc7a096d3bd3643"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/JOxvOTgpuCAhueGrJVdZfQ_87.0.4260.0/87.0.4260.0_chrome_installer.exe#/dl.7z",
