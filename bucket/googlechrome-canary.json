{
    "version": "77.0.3862.0",
    "description": "Fast, secure, and free web browser, built for the modern web.",
    "homepage": "https://www.google.com/chrome/canary",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/release2/chrome/AOTO4LLMxqvI-IO4pvhQTpM_77.0.3862.0/77.0.3862.0_chrome_installer.exe#/dl.7z",
            "hash": "1c2a2fd3fe8c2b076b565c6d1da5a1b44b81711d9edd7e4f9db00befe6ed5a87"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/PuZ1mveTbSSFCu633NJf1A_77.0.3862.0/77.0.3862.0_chrome_installer.exe#/dl.7z",
            "hash": "6728e73e128f7eff835e4bc2ad892fdbdd6cc2b36fb36eb86aacd19adfb4c0f9"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\chrome.7z\" \"$dir\" -Removal",
            "Move-Item \"$dir\\Chrome-bin\\*\" \"$dir\"",
            "Remove-Item \"$dir\\Chrome-bin\" -Force -Recurse"
        ]
    },
    "bin": [
        [
            "chrome.exe",
            "chrome-canary"
        ]
    ],
    "shortcuts": [
        [
            "chrome.exe",
            "Google Chrome (Canary)"
        ]
    ],
    "checkver": {
        "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
        "regex": "(?sm)<canary32><version>(?<version>[\\d.]+)</version>.+release2/chrome/(?<32>[\\w-]+)_.+<canary64>.+release2/chrome/(?<64>[\\w-]+)_.+</canary64>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/release2/chrome/$match64_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/canary64[version='$version']/sha256"
                }
            },
            "32bit": {
                "url": "https://dl.google.com/release2/chrome/$match32_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/canary32[version='$version']/sha256"
                }
            }
        }
    }
}
