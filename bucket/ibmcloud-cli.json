        },
        "32bit": {
            "url": "https://clis.ng.bluemix.net/download/bluemix-cli/0.20.0/win32/archive#/dl.zip",
            "hash": "sha1:a08073266f2dd740e502c18cc3fbad86f91f49f0"
        }
    },
    "extract_dir": "IBM_Cloud_CLI",
    "bin": [
        "ibmcloud.exe",
        [
