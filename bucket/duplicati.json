    "pre_uninstall": [
        "if (Get-Service -Name 'Duplicati' -ErrorAction 'SilentlyContinue') {",
        "    info 'Uninstalling service...'",
        "    if (!(is_admin)) { error 'Admin rights required to remove Duplicati service'; break }",
