{
    "version": "0.5.7.8",
    "description": "Mouse gesture recognition utility for Windows which allows you to create powerful mouse gestures that save you time.",
    "homepage": "https://www.strokesplus.net",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.strokesplus.net/FAQ"
    },
