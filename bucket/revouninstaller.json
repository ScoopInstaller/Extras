{
    "version": "2.2.0",
    "description": "Software uninstaller with support for additional cleanup of registries/files",
    "homepage": "https://www.revouninstaller.com/products/revo-uninstaller-free/",
    "license": "Freeware",
    "url": "https://www.revouninstaller.com/download-free-portable.php#/dl.zip",
    "hash": "f543d9e0627f4170674310f5759498bc65a08446c3c2693f9771493c9d3063f5",
    "extract_dir": "RevoUninstaller_Portable",
    "pre_install": [
        "'ctrlbars.dat', 'un_report.dat', 'settings.ini', 'winapp_report.dat' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\$_\")) {",
        "        Write-Host 'File' $_ 'does not exists. Creating.' -f Yellow",
        "        Set-Content \"$dir\\$_\" $content -Encoding Ascii",
        "    }",
        "}",
        "(Get-Content \"$dir\\settings.ini\") -replace 'AU on startup=1','AU on startup=0' | Set-Content \"$dir\\settings.ini\""
    ],
    "shortcuts": [
        [
            "RevoUPort.exe",
            "Revo Uninstaller"
        ]
    ],
    "persist": [
        "ctrlbars.dat",
        "un_report.dat",
        "settings.ini",
        "winapp_report.dat"
    ],
    "checkver": "Version: ([\\d.]+)"
}
