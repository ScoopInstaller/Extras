    "license": "Apache-2.0",
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.apache.org/tomcat/tomcat-10/v10.1.10/bin/apache-tomcat-10.1.10-windows-x64.zip",
            "hash": "sha512:0a73ba3afc1ea88425fbbc67a51279300c0c5b19ff794c8f63027a49f92dc4078cd252a90fa98a832a2a478b77d0f67445c828d163e6304fd81a5e1f3e9c5ecd"
        },
        "32bit": {
            "url": "https://downloads.apache.org/tomcat/tomcat-10/v10.1.10/bin/apache-tomcat-10.1.10-windows-x86.zip",
            "hash": "sha512:0e51ddda2478c1bf5621bf28f530aaf96abac43c3a845372252bd705110f27f77ddaa07b190e40eafed8fd9128c1f4d7e74a570d552290b059d427ca7a47266a"
        }
    },
    "extract_dir": "apache-tomcat-10.1.10",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "persist": [
        "conf",
        "webapps"
    ],
    "checkver": {
        "url": "https://downloads.apache.org/tomcat/tomcat-10/?C=M;O=D",
        "regex": "v([\\d.]+)/"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.apache.org/tomcat/tomcat-$majorVersion/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            },
            "32bit": {
                "url": "https://downloads.apache.org/tomcat/tomcat-$majorVersion/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        },
        "extract_dir": "apache-tomcat-$version"
    }
}
