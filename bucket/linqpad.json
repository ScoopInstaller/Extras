    "architecture": {
        "64bit": {
            "bin": [
                [
                    "LPRun9-x64.exe",
                    "lprun"
                ],
                [
                    "LINQPad9-x64.exe",
                    "linqpad"
                ],
                [
                    "LPRun9-x86.exe",
