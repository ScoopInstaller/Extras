{
    "version": "90.0.1",
    "description": "Popular open source web browser",
    "homepage": "https://www.mozilla.org/firefox/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/90.0.1/win64/en-US/Firefox%20Setup%2090.0.1.exe#/dl.7z",
            "hash": "sha512:1c9b581257f0f165ed8a82eecc1f0fe3d87da4011ca697a0324a0cc65af964ccebb2e834f0f0adb6c765e24d23ad3e9ba49f48c140125bd42803148ed663ccf2"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/90.0.1/win32/en-US/Firefox%20Setup%2090.0.1.exe#/dl.7z",
            "hash": "sha512:834faf0c4d48331f46faaabcbb4025d16c1b67f81dbf01f870ec91350f8a1c2e0c2001a170ca0bc24c0e37355c790856df19503e2f4b34b6f5c8e47c0b3a594d"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/en-US/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
