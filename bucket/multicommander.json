            "32bit": {
                "url": "http://multicommander.com/files/updates/MultiCommander_win32_Portable_($version).zip"
            }
        },
        "hash": {
            "url": "http://multicommander.com/downloads",
            "find": "$basename.+([A-Fa-f\\d]{40})"
        }
    }
