        [
            "Lepton.exe",
            "Lepton"
        ]
    ],
    "checkver": {
        "github": "https://github.com/hackjutsu/Lepton"
    },
    "autoupdate": {
        "url": "https://github.com/hackjutsu/Lepton/releases/download/v$version/Lepton-Setup-$version.exe#/dl.7z",
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512: $base64"
        }
    }
