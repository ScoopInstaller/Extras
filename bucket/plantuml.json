    "homepage": "http://plantuml.com/",
    "license": "GPL-3.0-only",
    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "url": "https://downloads.sourceforge.net/project/plantuml/1.2022.1/plantuml.1.2022.1.jar#/plantuml.jar",
    "hash": "sha1:ac9847dac6687f5079793952cf981f8d75ff4515",
    "pre_install": "Set-Content \"$dir\\plantuml.bat\" \"@javaw.exe -jar \"\"%~dp0plantuml.jar\"\"\" -Encoding Ascii",
