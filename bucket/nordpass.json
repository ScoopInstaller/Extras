    "license": {
        "identifier": "Freeware",
        "url": "https://my.nordaccount.com/legal/terms-of-service/nordpass/"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.npass.app/windows/NordPassSetup.exe#/dl.7z",
            "hash": "sha512:cff337f2d1a3746812aa7dcccab00cc8ce49c9dff56d3a79a7bf1ca1ef62f5404755ae174fd265c3d700a0ed42d6bd7c05710df82687a361d7ee9d45ae39d9f3",
