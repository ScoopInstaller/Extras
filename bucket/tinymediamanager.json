{
    "version": "5.1.7",
    "description": "tinyMediaManager is a media management tool written in Java/Swing. It is written to provide metadata for the Kodi Media Center (formerly known as XBMC), MediaPortal and Plex media server",
    "homepage": "https://www.tinymediamanager.org/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/tinyMediaManager/tinyMediaManager/blob/master/LICENSE"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://release.tinymediamanager.org/v5/dist/tinyMediaManager-5.1.7-windows-amd64.zip",
            "hash": "e5b88fa71e5714c6a80c2f87c0f3e2233b6ed5ed7cbebf9b5f291b7427fa6de1"
        }
    },
    "bin": "tinyMediaManagerCMD.exe",
    "shortcuts": [
        [
            "tinyMediaManager.exe",
            "tinyMediaManager"
        ]
    ],
    "persist": [
        "logs",
        "data",
        "cache",
        "backup"
    ],
    "checkver": {
        "url": "https://release.tinymediamanager.org/",
        "regex": "tmm_([\\d\\.]+)_windows-amd64\\.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://release.tinymediamanager.org/v5/dist/tinyMediaManager_$version_windows-amd64.zip",
                "hash": {
                    "url": "$url.sha256"
                }
            }
        }
    }
}
