    "pre_install": [
        "if (!(Test-Path \"$dir\\mp3tag.cfg\")) {",
        "   New-Item -ItemType File \"$dir\\mp3tag.cfg\" | Out-Null",
        "}"
    ],
    "post_install": [
        "Remove-Item \"$dir\\`$PLUGINSDIR\" -Recurse",
        "Remove-Item \"$dir\\`$R0\"",
        "Remove-Item \"$dir\\Mp3tagUninstall.exe.nsis\""
    ]
}
