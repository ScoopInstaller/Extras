    "bin": "mp3tag.exe",
    "shortcuts": [
        [
            "mp3tag.exe",
            "Mp3tag"
        ]
    ],
    "persist": [
        "data",
        "export",
        "mp3tag.cfg"
    ],
    "checkver": "Mp3tag v([\\d\\w.]+)",
    "autoupdate": {
        "url": "https://download.mp3tag.de/mp3tagv$cleanVersionsetup.exe#dl.7z"
