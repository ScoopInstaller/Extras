    "extract_dir": "Status",
    "pre_install": "if (!(Test-Path \"$env:LOCALAPPDATA\\Status\")) { New-Item \"$env:LOCALAPPDATA\\Status\" -ItemType 'Directory' | Out-Null }",
    "bin": "Status.exe",
    "shortcuts": [
