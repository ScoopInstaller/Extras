    "autoupdate": {
        "url": "https://freecommander.com/downloads/FreeCommanderXE-32_portable$minorVersion.zip",
        "hash": {
            "url": "https://freecommander.com/en/downloads-portable/",
            "regex": "$basename.*?SHA256:\\s+$sha256"
