{
    "version": "1.0.9211",
    "description": "Group Chat That's All Fun & Games",
    "homepage": "https://discord.com",
    "license": {
        "identifier": "Freeware",
        "url": "https://discord.com/terms"
    },
    "notes": "User may need to re-enable startup in Discord settings",
    "architecture": {
        "64bit": {
            "url": "https://stable.dl2.discordapp.net/distro/app/stable/win/x64/1.0.9211/DiscordSetup.exe#/dl.7z",
            "hash": "bec671f0b3236bc072095300584c2def8fc9e4b7af4a0896cc9f1430e8cb6317"
        }
    },
    "pre_install": [
        "Expand-7zipArchive \"$dir\\Discord-$version-full.nupkg\" -ExtractDir 'lib\\net45' -DestinationPath \"$dir\\app-$version\" -Removal",
        "Move-Item \"$dir\\app-$version\\installer.db\" \"$dir\"",
        "Move-Item \"$dir\\app-$version\\app.ico\" \"$dir\"",
        "New-Item \"$dir\\packages\" -ItemType Directory | Out-Null",
        "Move-Item \"$dir\\RELEASES\" \"$dir\\packages\""
    ],
    "post_install": [
        "if (!(Test-Path \"$env:APPDATA\\discord\") -and (Test-Path \"$persist_dir\\data\")) {",
        "    info '[Official Mode]: Copying user data from portapps data directory . . .'",
        "    New-Item \"$env:APPDATA\\discord\" -ItemType Directory | Out-Null",
        "    Copy-Item \"$persist_dir\\data\\*\" \"$env:APPDATA\\discord\" -Recurse",
        "}",
        "if (Test-Path \"$bucketsdir\\$bucket\\scripts\\$app\\Discord.lnk\") {",
        "    Copy-Item \"$bucketsdir\\$bucket\\scripts\\$app\\Discord.lnk\" \"$env:APPDATA\\Microsoft\\Windows\\Start Menu\\Programs\"",
        "}"
    ],
    "shortcuts": [
        [
            "Update.exe",
            "Discord",
            "--processStart Discord.exe",
            "app.ico"
        ]
    ],
    "post_uninstall": "Remove-Item \"$env:APPDATA\\Microsoft\\Windows\\Start Menu\\Programs\"\\Discord.lnk\"",
    "checkver": {
        "url": "https://updates.discord.com/distributions/app/manifests/latest?channel=stable&platform=win&arch=x64",
        "regex": "x64/([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://stable.dl2.discordapp.net/distro/app/stable/win/x64/$version/DiscordSetup.exe#/dl.7z"
            }
        }
    }
}
