{
    "version": "0.9.9",
    "description": "a tiling window manager for Windows 10",
    "homepage": "https://www.workspacer.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://workspacer.blob.core.windows.net/installers/stable/workspacer-stable-latest.msi"
        }
    },
    "extract_dir": "workspacer",
    "bin": [
        "workspacer.exe"
    ,
    "shortcuts": [
        [
            "workspacer.exe",
            "workspacer"
        ]
    ],
    "checkver": {
        "url": "https://raw.githubusercontent.com/rickbutton/workspacer/master/VERSION",
        "regex": "(\d+)\.(\d+)\.(\d+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://workspacer.blob.core.windows.net/installers/stable/workspacer-stable-latest.msi"
            }
        }
    }
}
