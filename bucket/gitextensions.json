        [
            "GitExtensions.exe",
            "gite"
        ]
    ],
    "shortcuts": [
        [
            "GitExtensions.exe",
            "Git Extensions"
        ]
    ],
    "persist": "GitExtensions.settings",
    "checkver": {
        "url": "https://api.github.com/repos/gitextensions/gitextensions/releases",
        "regex": "(?sm)browser_download_url.*?releases/download/v(?<tag>([\\d.]+))/GitExtensions-Portable-(?<version>([\\d.]+))-(?<commit>[\\w]+)\\.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "url": "https://github.com/gitextensions/gitextensions/releases/download/v$matchTag/GitExtensions-Portable-$matchVersion-$matchCommit.zip"
    }
