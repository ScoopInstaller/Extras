{
    "version": "2.19.1",
    "description": "Font manager",
    "homepage": "https://fontba.se",
    "license": {
        "identifier": "Freeware",
        "url": "https://fontba.se/legal/terms"
    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.19.1.exe#/dl.7z",
            "hash": "sha512:e3e552fd18e34262de5c66dd05c9915c5817a21ab02d3373963777f2c308344ae25eedeaf8129e62e7e3dafa2d0f10f00554f650e70e9ae4cbbfaae0ec36e7f4",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
    "checkver": {
        "url": "https://fontba.se/updates",
        "regex": "([\\d.]+)</a"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://releases.fontba.se/win/FontBase-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
