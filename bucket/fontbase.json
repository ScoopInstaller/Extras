    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.25.0.exe#/dl.7z",
            "hash": "sha512:5c6dfa18446fd8b9e794e0eb7ee33d526c76aeaa006fa1d828932ae39cbc81741c635fb518da90180ab9bc85374f43cea7db6d6868d7c95b665463be15c4972e",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
    "checkver": {
        "url": "https://releases.fontba.se/win/latest.yml",
        "regex": "(?m)^version:\\s*([0-9]+(?:\\.[0-9]+)*)\\s*$"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://releases.fontba.se/win/FontBase-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
