    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.22.4.exe#/dl.7z",
            "hash": "sha512:552901ea9ed3a8c79df2a4708b7f67f659b5406f49d01c88dc0524f842097e6d9502b03d882992569c982c49476b7f13f728ac011a99a71056bb9c04e62e5386",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
