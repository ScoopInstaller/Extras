    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.17.17.exe#/dl.7z",
            "hash": "sha512:5673aaddf45cd64b57dd9ea5521a1c77bc5c1b86836f6ea3226fa88c9ba23c9fa0fbdd11c26ccd4945b623ec267f872abbec9162f84a247787b0c3c1cd7cbc1b",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
