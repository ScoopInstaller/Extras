    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.23.1.exe#/dl.7z",
            "hash": "sha512:966d39768520bce38f6f2b2218c52652e2d4f75cee22eacc3bdc479650cd7a56a165f49f4803c64ee45b3739dffc296eff0da2ca0ee12092bd8f141e5c4f918c",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
    "checkver": {
