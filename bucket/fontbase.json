    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.16.4.exe#/dl.7z",
            "hash": "sha512:6aa31f9b7b44b466fe7ef984394322a6b331693f34d7ad74a68295c065d9d23e45ea26f29be019aee891fc5d043fc3b4f702393c51fd915f0c8fecdd8979cdf1",
            "pre_install": [
