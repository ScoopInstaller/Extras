{
    "version": "2.16.0",
    "description": "Font manager",
    "homepage": "https://fontba.se",
    "license": {
        "identifier": "Freeware",
        "url": "https://fontba.se/legal/terms"
    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.16.0.exe#/dl.7z",
            "hash": "sha512:5ac5578ef0b21c00741dd928a7f83b03daa397e15a52e22bb0c7e2be4f65bbd9b93584366beb86251a815ec887c595776e5df5b7fd835313f8beaf7781020fb6",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
    "checkver": {
        "url": "https://fontba.se/updates",
        "regex": "([\\d.]+)</a"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://releases.fontba.se/win/FontBase-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
