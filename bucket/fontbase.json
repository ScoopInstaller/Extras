{
    "version": "2.16.8",
    "description": "Font manager",
    "homepage": "https://fontba.se",
    "license": {
        "identifier": "Freeware",
        "url": "https://fontba.se/legal/terms"
    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.16.8.exe#/dl.7z",
            "hash": "sha512:1c52796d84d73b428f8da91dd05debe059deecc3a0fc5c67662ff0e105e29ab8ba9a86778efdc804bb6b090284e04e6e4822874f5c85bdea5062cb6a03cae55f",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
