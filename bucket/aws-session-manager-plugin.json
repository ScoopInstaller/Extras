{
    "version": "1.2.54.0",
    "description": "This plugin helps you to use the AWS Command Line Interface (AWS CLI) to start and end sessions to your managed instances",
    "homepage": "https://github.com/aws/session-manager-plugin",
    "license": "Apache-2.0",
    "url": "https://s3.amazonaws.com/session-manager-downloads/plugin/1.2.54.0/windows/SessionManagerPluginSetup.exe",
    "hash": "91f578f1c30dc4b0ab3af642ef5c9c20586e3dd436775a9ea7c77fa4ed734813",
    "bin": "session_manager_plugin.exe",
    "architecture": {
        "32bit": {
            "installer": {
                "script": [
                    "Expand-DarkArchive \"$dir\\$fname\" \"$dir\\extracted\" -Removal",
                    "Expand-MsiArchive \"$dir\\extracted\\AttachedContainer\\SessionManagerCLI_MSI_32.msi\" \"$dir\"",
                    "Remove-Item \"$dir\\extracted\" -Force -Recurse"
                ]
            }
        },
        "64bit": {
            "installer": {
                "script": [
                    "Expand-DarkArchive \"$dir\\$fname\" \"$dir\\extracted\" -Removal",
                    "Expand-MsiArchive \"$dir\\extracted\\AttachedContainer\\SessionManagerCLI_MSI_64.msi\" \"$dir\"",
                    "Remove-Item \"$dir\\extracted\" -Force -Recurse"
                ]
            }
        }
    }
}
