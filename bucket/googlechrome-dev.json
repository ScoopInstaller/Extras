{
    "version": "78.0.3876.0",
    "description": "Fast, secure, and free web browser, built for the modern web.",
    "homepage": "https://www.google.com/chrome/dev",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.google.com/chrome/privacy/eula_text.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/release2/chrome/AOvC_J25nP1H5u6-zfbb-DM_78.0.3876.0/78.0.3876.0_chrome_installer.exe#/dl.7z",
            "hash": "29d604e2322bdf11b0c16a23bed0d1de5b376f3ea250eea413d5b877cac17bfb"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/OyeSBHAGkGLX1V78miNbqQ_78.0.3876.0/78.0.3876.0_chrome_installer.exe#/dl.7z",
            "hash": "41c35a5651f7c95f650faa64e836da94c0c0710315b516d6b31822085c11692b"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\chrome.7z\" \"$dir\" -Removal",
            "Move-Item \"$dir\\Chrome-bin\\*\" \"$dir\"",
            "Remove-Item \"$dir\\Chrome-bin\" -Force -Recurse"
        ]
    },
    "bin": [
        [
            "chrome.exe",
            "chrome-dev"
        ]
    ],
    "shortcuts": [
        [
            "chrome.exe",
            "Google Chrome (Dev)"
        ]
    ],
    "checkver": {
        "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
        "regex": "(?sm)<dev32><version>(?<version>[\\d.]+)</version>.+release2/chrome/(?<32>[\\w-]+)_.+<dev64>.+release2/chrome/(?<64>[\\w-]+)_.+</dev64>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/release2/chrome/$match64_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/dev64[version='$version']/sha256"
                }
            },
            "32bit": {
                "url": "https://dl.google.com/release2/chrome/$match32_$version/$version_chrome_installer.exe#/dl.7z",
                "hash": {
                    "url": "https://lab.skk.moe/chrome/api/chrome.min.xml",
                    "xpath": "/chromechecker/dev32[version='$version']/sha256"
                }
            }
        }
    }
}
