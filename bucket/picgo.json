    "description": "Image uploader/manager",
    "homepage": "https://picgo.github.io/PicGo-Doc/en/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/Molunerfinn/PicGo/releases/download/v2.3.0-beta.7/PicGo-Setup-2.3.0-beta.7-x64.exe#/dl.7z",
            "hash": "sha512:cace53acc6e928598965794435654e994d85d7937ff20101e96967ed0f206d0f875845e9f650c41cd725a68886caaca3b148b9d9f36a3cb702a2ea1a7208c148",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Recurse"
            ]
        }
    },
    "bin": "PicGo.exe",
    "shortcuts": [
        [
