            "hash": "566eecbfee64e12d5cda0005a0e8e8812cfcee79e8956fa84ab15acad21cfe55",
            "installer": {
                "file": "autohotkey-installer.exe",
                "args": [
                    "/S",
                    "/x64",
                    "/uiAccess=0",
                    "/IsHostApp=1",
                    "/D=$dir"
                ],
                "keep": true
            },
            "uninstaller": {
                "file": "autohotkey-installer.exe",
                "args": "/Uninstall"
            },
            "bin": [
                [
                    "autohotkeyu64.exe",
                    "autohotkey"
                ],
                "compiler/ahk2exe.exe"
            ]
        },
        "32bit": {
