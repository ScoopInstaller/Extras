    "pre_install": [
        "if(!(Test-Path \"$persist_dir\\ToDoList.ini\")) { New-Item \"$dir\\ToDoList.ini\" -ItemType \"file\" -Value \"\" }",
        "if(!(Test-Path \"$persist_dir\\TDLTransEdit.ini\")) { New-Item \"$dir\\TDLTransEdit.ini\" -ItemType \"file\" -Value \"\" }"
    ],
    "bin": [
        "ToDoList.exe",
        "TDLTransEdit.exe"
    ],
    "shortcuts": [
        [
            "ToDoList.exe",
            "ToDoList"
        ],
        [
            "TDLTransEdit.exe",
            "ToDoList Translation Editor"
        ]
    ],
    "persist": [
        "ToDoList.ini",
        "TDLTransEdit.ini",
        "Resources\\TaskLists"
    ],
    "checkver": {
        "url": "https://filehippo.com/download_todolist/",
        "regex": "ToDoList\\s+([\\d.]+)"
    },
    "autoupdate": {
        "url": "http://abstractspoon.pbwiki.com/f/todolist_exe.zip"
    }
