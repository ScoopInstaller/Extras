{
    "homepage": "http://plantuml.com/",
    "version": "1.2017.16",
    "license": "GPLv2",
    "url": "https://sourceforge.net/projects/plantuml/files/1.2017.16/plantuml.1.2017.16.jar",
    "hash": "md5:e7828bb00c700fda9161b70abf4f7978",
    "bin": [
        "plantuml.bat"
    ],
    "pre_install": "\"@java -jar \"\"$dir\\plantuml.$version.jar\"\" -graphvizdot \"\"$(shimdir $global)\\dot.exe\"\" %*\" | out-file -en oem \"$dir\\plantuml.bat\"",
    "depends": "graphviz",
    "suggest": {
        "JDK": [
            "extras/oraclejdk",
            "openjdk"
        ]
    },
    "checkver": {
        "url": "http://plantuml.com/download",
        "re": "PlantUML compiled Jar \\(Version ([\\d.]+)\\)"
    },
    "autoupdate": {
        "url": "https://sourceforge.net/projects/plantuml/files/$version/plantuml.$version.jar",
        "hash": {
            "mode": "extract",
            "url": "https://sourceforge.net/projects/plantuml/rss?path=/$version",
            "find": "/(?:$basename)/download\" filesize=\"(?<size>[\\d]+)\"><media:hash algo=\"md5\">([a-fA-F0-9]{32})</media:hash>"
        }
    }
}
