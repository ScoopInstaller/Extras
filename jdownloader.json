{
    "homepage": "http://jdownloader.org/",
    "description": "JDownloader is a free, open-source download management tool",
    "version": "2",
    "url": [
        "http://installer.jdownloader.org/JDownloader.jar",
        "http://jdownloader.org/lib/tpl/arctic/images/favicon.ico#icon.ico"
    ]
    "suggest": "oraclejre",
    "persist": [
        "cfg"
    ],
    "bin": "JDownloader.jar",
    "installer": {
        "script": [
            "\"start javaw.exe -jar JDownloader.jar\" | Out-File -Encoding ASCII \"$dir\\JDownloader.bat\""
        ]
    },
    "shortcuts": [
        [
            "JDownloader.bat",
            "JDownloader 2",
            "",
            "icon.ico"
        ]
    ]
}
